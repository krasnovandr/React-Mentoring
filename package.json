{
  "name": "reactmentoring",
  "version": "1.0.0",
  "description": "React Mentoring App",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krasnovandr/React-Mentoring.git"
  },
  "keywords": [
    "React",
    "Mentoring"
  ],
  "author": "AndreiKrasnou",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krasnovandr/React-Mentoring/issues"
  },
  "homepage": "https://github.com/krasnovandr/React-Mentoring#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "express": "^4.16.4",
    "query-string": "^6.4.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "use-react-router": "^1.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./setupJest.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "coveragePathIgnorePatterns": [
      "index.js",
      "App.js"
  ],
    "automock": false
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jest-fetch-mock": "^2.1.1",
    "react-hot-loader": "^4.8.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^6.0.3",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.2.1"
  }
}