    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-hot-server-middleware": "^0.6.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
