  "bugs": {
    "url": "https://github.com/krasnovandr/React-Mentoring/issues"
  },
  "homepage": "https://github.com/krasnovandr/React-Mentoring#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@material-ui/core": "^3.9.2",
    "@types/jest": "^24.0.13",
    "@types/material-ui": "^0.21.6",
    "@types/node": "^12.0.2",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
