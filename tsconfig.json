{
  "compilerOptions": {
    "outDir": ".public/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es6",
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "sourceMap": true,
    "alwaysStrict": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "typeRoots": [ "./types"]
    
  },
  "exclude": ["node_modules"],
}
